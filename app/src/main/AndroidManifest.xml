<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.job">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/letter"
        android:label="@string/app_name"
        android:roundIcon="@drawable/letter"
        android:supportsRtl="true"
        android:theme="@style/Theme.JOB"
        tools:ignore="MissingClass">
        <activity
            android:name=".SavedJobsActivity"
            android:exported="true" />
        <activity
            android:name=".Applied_Jobs_Activity"
            android:exported="true" />

        <activity
            android:name=".Admin_panel_Activity"
            android:exported="true" />
        <activity
            android:name=".Sign_Up_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sign_In_Activity"
            android:exported="true" />
        <activity
            android:name=".Add_job_Info_Activity"
            android:exported="false" />
        <activity
            android:name=".Job_Details_Activity"
            android:exported="false" />
        <activity android:name=".Find_Jobs_Activity" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>